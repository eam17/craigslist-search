import csv

points = {}
with open('result.csv', mode='r') as infile:
    reader = csv.reader(infile)
    for row in reader:
        points[row[0]] = row[1:]

del points['URL']
print(points)
# 'https://denver.craigslist.org/gms/d/denver-hugh-3-house-garage-sale/7196939589.html': ['Lakewood co', '39.689500', '-105.090800'],